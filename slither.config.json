{
  "filter_paths": "(node_modules|lib|build|forge-test|test|contracts/legacy)",
  "exclude_dependencies": true,
  "exclude_informational": true,
  "exclude_low": true,
  "exclude_optimization": true,
  "solc_remaps": [
    "@openzeppelin/=node_modules/@openzeppelin/"
  ],
  "detectors_to_exclude": [
    "reentrancy-no-eth",
    "reentrancy-benign",
    "reentrancy-events",
    "divide-before-multiply",
    "mapping-deletion",
    "uninitialized-local",
    "unused-return",
    "encode-packed-collision"
  ]
}
