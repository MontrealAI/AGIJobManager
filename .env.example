# Copy this file to .env and fill in values (never commit real secrets).
# PRIVATE_KEYS is only used for deployment/verification workflows.

# Deployment keys (comma-separated, no spaces, keep local)
PRIVATE_KEYS=0xabc...,0xdef...

# RPC provider keys (used to build sepolia/mainnet URLs)
ALCHEMY_KEY=your_alchemy_key_for_sepolia
ALCHEMY_KEY_MAIN=your_alchemy_key_for_mainnet
INFURA_KEY=optional_infura_key

# Direct RPC URL overrides (optional; take precedence if set)
SEPOLIA_RPC_URL=
MAINNET_RPC_URL=

# Etherscan verification
ETHERSCAN_API_KEY=your_etherscan_api_key

# Provider polling
RPC_POLLING_INTERVAL_MS=8000

# Gas + confirmations tuning (optional)
SEPOLIA_GAS=8000000
MAINNET_GAS=8000000
SEPOLIA_GAS_PRICE_GWEI=
MAINNET_GAS_PRICE_GWEI=
SEPOLIA_CONFIRMATIONS=2
MAINNET_CONFIRMATIONS=2
SEPOLIA_TIMEOUT_BLOCKS=500
MAINNET_TIMEOUT_BLOCKS=500

# Compiler settings (optional)
SOLC_VERSION=0.8.33
SOLC_RUNS=200
SOLC_VIA_IR=true
SOLC_EVM_VERSION=london

# Local test chain
GANACHE_MNEMONIC=test test test test test test test test test test test junk

# Post-deploy config / verification (optional; used by scripts/postdeploy-config.js and scripts/verify-config.js)
AGIJOBMANAGER_ADDRESS=
EXPECTED_OWNER=
NEW_OWNER=
AGI_TOKEN_ADDRESS=
ENS_ADDRESS=
NAME_WRAPPER_ADDRESS=
CLUB_ROOT_NODE=
AGENT_ROOT_NODE=
VALIDATOR_MERKLE_ROOT=
AGENT_MERKLE_ROOT=
REQUIRED_VALIDATOR_APPROVALS=
REQUIRED_VALIDATOR_DISAPPROVALS=
PREMIUM_REPUTATION_THRESHOLD=
VALIDATION_REWARD_PERCENTAGE=
MAX_JOB_PAYOUT=
JOB_DURATION_LIMIT=
COMPLETION_REVIEW_PERIOD=
DISPUTE_REVIEW_PERIOD=
ADDITIONAL_AGENT_PAYOUT_PERCENTAGE=
TERMS_IPFS_HASH=
CONTACT_EMAIL=
ADDITIONAL_TEXT1=
ADDITIONAL_TEXT2=
ADDITIONAL_TEXT3=
MODERATORS=0x...
ADDITIONAL_VALIDATORS=0x...
ADDITIONAL_AGENTS=0x...
BLACKLISTED_AGENTS=0x...
BLACKLISTED_VALIDATORS=0x...
AGI_TYPES=[{\"nftAddress\":\"0x...\",\"payoutPercentage\":50}]
CONFIG_PATH=ops-config.json
